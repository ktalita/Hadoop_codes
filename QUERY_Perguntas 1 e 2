SELECT
REGIAO,MES,A.ANO,PRECO_MEDIO_REVENDA,SALARIO_MINIMO,
PRECO_MEDIO_REVENDA/SALARIO_MINIMO*100 AS PERC_SAL_MIN_L,
PRECO_MEDIO_REVENDA*45/SALARIO_MINIMO*100 AS PERC_SAL_MIN_TANQUE
FROM
PRECO_COMBUSTIVEL_md A
JOIN salario_minimo_md B
ON A.ANO = B.ANO
WHERE
PRODUTO = 'GASOLINA COMUM'
ORDER BY A.ANO,B.MES
